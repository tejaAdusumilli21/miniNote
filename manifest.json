{
  "name": "Mini Note",
  "description": "Offline notepad with collections, pages, Insert and Draw ribbon.",
  "version": "0.3.1",
  "author": "Teja Adusumilli",
  "manifest_version": 3,
  "action": {
    "default_title": "Open Mini Note",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png",
      "256": "icons/icon256.png"
    }
  },
  "background": {
    "service_worker": "service_worker.js"
  },
  "permissions": [
    "downloads",
    "storage",
    "tabs"
  ],
  "host_permissions": [],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png"
  }
}